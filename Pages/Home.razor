  #region Fields
  
  #region Readonly Fields
  
  /// <summary>
  /// The time, in milliseconds, to show the feedback text for.
  /// </summary>
  readonly int feedbackTextTimeout = 1000;
  
  #endregion
  
  #endregion
  
  #region Initialisation

  /// <summary>
  /// Initialise the page.
  /// </summary>
  /// <param name="firstRender"> Whether this is the first render. </param>
  protected override async Task OnAfterRenderAsync(bool firstRender)
  {
    if (firstRender)
    {
      // 15 is approximately 60 frames a second, whilst 30 is approximately 30 frames a second.
      const int animationSpeed = 15;

      // Delay inputs so that the user doesn't scroll too quickly through the content of the site.
      const int inputDelay = 750;

      await JSRuntime.InvokeVoidAsync("initialiseCursor", "cursor", "to-diamond-animation", "to-pentagon-animation", "pointer-target", animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseSVGTrailEffect", "menu-header", 2, true, false, animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseSVGTrailEffect", "hero-section-header", 10, true, false, animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseScrollOverride", "scroll-point", "sticky-scroll-point", "skip-forwards-scroll-point", "skip-backwards-scroll-point", "skip-horizontal-scroll-point", "skip-vertical-scroll-point", "scroll-in-animation", "scroll-out-animation", inputDelay);
      await JSRuntime.InvokeVoidAsync("initialiseLinesHoverEffect", "line-container", animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseLinesHoverEffect", "line-container", "menu", animationSpeed);
      //await JSRuntime.InvokeVoidAsync("initialiseRevealTextEffect", "reveal-text", animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseSplitTextLateralEffect", "about-split-text-horizontal", "about-split-text-top-horizontal", "about-split-text-bottom-horizontal", 5, animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseSplitTextLateralEffect", "about-split-text-vertical", "about-split-text-top-vertical", "about-split-text-bottom-vertical", 5, animationSpeed);
      await JSRuntime.InvokeVoidAsync("initialiseKeyCopying", "contact-button", "selectable-contact-text", "feedback-text-in-animation", "feedback-text-out-animation", feedbackTextTimeout);
    }
  }
  
  #endregion
